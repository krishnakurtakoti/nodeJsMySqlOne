<!DOCTYPE html>
<html>
<head>
  <title>AddingVendor_BillHistory</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
<link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
</head>
<body>


<div class=" tab-pane" id="bill" role="tabpanel">
  <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12 m-b-60">
    <table class="table table-xl">
      <thead>
        <tr>
          <th>vendorId</th>
          <th>stockNumberId</th>
          <th>isAddToBill</th>
          <th>Date</th>
          <th>purchaseDate</th>
          <th>Date Due</th>
          <th>Date Paid</th>
          <th>Amount</th>
          <th>Invoice</th>
          <th>Status</th>
          <th>Check#</th>
          <th>Cleared</th>
          <th>Check Memo</th>
        </tr>
      </thead>
      <tbody>
        <% if(data.length){

        for(var i = 0; i< data.length; i++) {%>
        <tr>

            <td><%= data[i].vendorId%></td>
            <td><%= data[i].stockNumberId%></td>
            <td><%= data[i].isAddToBill%></td>
          <td><%= data[i].date%></td>
            <td><%= data[i].purchaseDate%></td>
          <td><%= data[i].dateDue%></td>
          <td><%= data[i].datePaid%></td>
          <td><%= data[i].invoice%></td>
          <td><%= data[i].status%></td>
          <td><%= data[i].checkNumber%></td>
          <td><%= data[i].isCleared%></td>
          <td><%= data[i].checkMemo%></td>


        </tr>
        <% }

         }else{ %>
             <tr>
                <td colspan="3">No user</td>
             </tr>
          <% } %>

      </tbody>
    </table>
  </div>

</body>
</html>
